{
  "$schema": "https://www.raycast.com/schemas/extension.json",
  "name": "notion-formulas",
  "title": "Notion Formulas",
  "description": "Documentation and examples for Notion's new formulas.",
  "icon": "icon.png",
  "author": "bensomething",
  "license": "MIT",
  "categories": [
    "Documentation"
  ],
  "commands": [
    {
      "name": "documentation",
      "title": "Search Documentation",
      "description": "Search functions, operators, and more.",
      "mode": "view"
    },
    {
      "name": "examples",
      "title": "Search Examples",
      "description": "Search your saved formula examples.",
      "mode": "view"
    },
    {
      "name": "create",
      "title": "Create Example",
      "description": "Create a new formula example snippet.",
      "mode": "view"
    }
  ],
  "preferences": [
    {
      "name": "notation",
      "title": "Notation Preference",
      "description": "Used for basic examples when pasted.",
      "type": "dropdown",
      "default": "regular",
      "data": [
        {
          "title":"Regular: length(\"Notion\")",
          "value":"regular"
        },
        {
          "title":"Dot: \"Notion\".length()",
          "value":"dot"
        }
      ],
      "required": true
    }
  ], 
  "dependencies": {
    "@raycast/api": "^1.47.2",
    "axios": "^1.3.1",
    "json2md": "^2.0.0",
    "lodash.groupby": "^4.6.0"
  },
  "devDependencies": {
    "@types/json2md": "^1.5.1",
    "@types/lodash.groupby": "^4.6.7",
    "@types/node": "~18.11.18",
    "@types/react": "^18.0.27",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "@typescript-eslint/parser": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-prettier": "^8.6.0",
    "react-devtools": "^4.27.1",
    "typescript": "^4.9.5"
  },
  "scripts": {
    "build": "ray build -e dist",
    "dev": "ray develop"
  }
}
